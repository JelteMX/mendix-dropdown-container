    "@types/jest": "^24.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-onclickoutside": "^6.7.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^4.13.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "dependencies": {
